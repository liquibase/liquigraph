<changelog>
    <changeset id="first-changelog" author="fbiville">
        <query>CREATE (:Person {name:'Edgar Poe'}),
                      (:Person {name:'Lorie'}),
                      (:Person {name:'Tommy'})
        </query>
    </changeset>
    <changeset id="second-changelog" author="team">
        <query>
            <template>MATCH (p:Person) WHERE p.name IN {names} SET p.age = 20</template>
            <parameters>
                <parameter name="names">
                    <value>Edgar Poe</value>
                    <value>Tommy</value>
                </parameter>
            </parameters>
        </query>
    </changeset>
</changelog>
